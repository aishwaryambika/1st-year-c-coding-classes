#include <stdio.h>

int main()
{
    int i,j,n;
    char a='A';
    char b='a';
    printf("Enter value of n:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=i;j++)
    {
        printf("%c%c",a,b);
        printf("\t");
        a++;
        b++;
    }
    printf("\n");
    }

    return 0;
}
